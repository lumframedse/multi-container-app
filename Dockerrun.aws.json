{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions":[
		{
			"name":"client",
			"image":"lumframedse/multi-client",
			"hostname":"client",
			"essential":false,
			"memory":128
		},
		{
			"name":"server",
			"image":"lumframedse/multi-server",
			"hostname":"api",
			"essential":false,
			"memory":128
		},
		{
			"name":"worker",
			"image":"lumframedse/multi-worker",
			"hostname":"worker",
			"essential":false,
			"memory":128
		},
		{
			"name":"nginx",
			"image":"lumframedse/multi-nginx",
			"hostname":"nginx",
			"essential":true,
			"portMappings":[
				{
					"hostPort":80,
					"containerPort":80
				}
			],
			"links":["client","server"],
			"memory":128
		}
	]
}